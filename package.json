{
  "name": "python-builds",
  "version": "1.0.0",
  "private": true,
  "license": "MIT",
  "repository": "https://github.com/viceice/python-builds.git",
  "author": "Michael Kriese <michael.kriese@visualon.de>",
  "scripts": {
    "compile": "run-p compile:*",
    "compile:builder": "cd src/builder && ncc build ./index.ts -s -o ../../dist/builder",
    "compile:releaser": "cd src/releaser && ncc build ./index.ts -s -o ../../dist/releaser",
    "eslint": "eslint . --ext .js,.mjs,.ts",
    "eslint:fix": "yarn eslint --fix",
    "lint": "run-s eslint prettier",
    "prepare": "run-s compile:*",
    "prettier": "prettier \"{**/*,*}.*\" --list-different",
    "prettier:fix": "prettier \"{**/*,*}.*\" --write",
    "start:builder": "run-s compile:builder && node dist/builder",
    "start:releaser": "run-s compile:releaser && node dist/releaser"
  },
  "dependencies": {
    "@actions/core": "1.2.4",
    "@actions/exec": "1.0.4",
    "@actions/github": "2.2.0",
    "@actions/io": "1.0.2",
    "@sindresorhus/is": "2.1.1",
    "chalk": "4.0.0",
    "find-up": "4.1.0",
    "got": "11.2.0",
    "lzma-native": "6.0.1",
    "renovate": "19.239.11",
    "semver": "7.3.2",
    "source-map-support": "0.5.19",
    "strip-ansi": "6.0.0",
    "tar": "6.0.2",
    "www-authenticate": "0.6.2"
  },
  "devDependencies": {
    "@types/fs-extra": "9.0.1",
    "@types/jest": "25.2.3",
    "@types/lzma-native": "4.0.0",
    "@types/node": "12.12.43",
    "@types/semver": "7.2.0",
    "@types/shelljs": "0.8.8",
    "@types/tar": "4.0.3",
    "@typescript-eslint/eslint-plugin": "2.34.0",
    "@typescript-eslint/parser": "2.34.0",
    "@zeit/ncc": "0.22.3",
    "eslint": "7.1.0",
    "eslint-config-prettier": "6.11.0",
    "eslint-plugin-import": "2.20.2",
    "eslint-plugin-promise": "4.2.1",
    "fs-extra": "9.0.0",
    "husky": "4.2.5",
    "jest": "25.5.4",
    "nock": "12.0.3",
    "npm-run-all": "4.1.5",
    "prettier": "2.0.5",
    "prettier-plugin-package": "1.0.0",
    "pretty-quick": "2.0.1",
    "shelljs": "0.8.4",
    "simple-git": "2.5.0",
    "ts-jest": "25.5.1",
    "typescript": "3.9.3"
  }
}
